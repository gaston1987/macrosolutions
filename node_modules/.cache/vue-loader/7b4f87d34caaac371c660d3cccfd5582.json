container>\n          </v-form>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n<script>\nexport default {\n  data () {\n\n    return {\n      ex1: { label: 'Monto', val: 0, color: 'blue' },\n      valid: true,\n      name: '',\n      nameRules: [\n        v => !!v || 'Nombre es requerido',\n        v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n      ],\n      email: '',\n      emailRules: [\n        v => !!v || 'E-mail es requerido',\n        v => /.+@.+\\..+/.test(v) || 'E-mail debe ser válido',\n      ],\n      select: null,\n      items: [\n        'Masculino',\n        'Femenino',\n        'Otro',\n        'Prefiero no decir',\n      ],\n      checkbox: false,\n      select2: null,\n      items2: [\n        '3',\n        '4',\n        '5',\n        '6',\n        '9',\n        '12',\n        '15',\n        '18',\n        '21',\n        '24',\n      ],\n    }\n  },\n  methods: {\n    validate () {\n      this.$refs.form.validate()\n    },\n    reset () {\n      this.$refs.form.reset()\n    },\n    resetValidation () {\n      this.$refs.form.resetValidation()\n    },\n  },\n}\n</script>"]}]}